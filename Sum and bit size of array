#include <iostream>
using namespace std;

int getSum(int arr[] , int n){
    
    
    
    int sum = 0;
    for(int i ; i <= n ; i++){
        sum = sum + arr[i];
    }
    return sum;
}

int main() {
    
    int arr[5] = {1,2,3,4,5};
    cout << "Sum is " << getSum(arr , 5) << endl;
    cout << "SIZE : "<< sizeof(arr) << endl;
    
    return 0;
}
