
#include <iostream>
#include <climits>
using namespace std;




int main() {
    
    int size;
    int num[10];
    cin >> size;
    int sum;
    
    for (int i=0 ; i<size ; i++){
        
        cin >> num[i];
        
       
        sum += num[i];
        
        
    }
    
    cout << sum << endl;
    
    

    return 0;
}
