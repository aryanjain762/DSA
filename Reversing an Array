#include <iostream>
#include <climits>

using namespace std;

int main() {
    int arr[] = {1,3,5,7,9,-2,-4,-6,-8,0};
    int size = sizeof(arr)/sizeof(arr[0]);
    int start = 0;
    int end = size - 1;
    
    while(start<=end){
        swap(arr[start],arr[end]);
        start++;
        end--;
        
    }
    
    cout << "Reversed Array is" << endl;
    
    for(int i = 0 ; i < size ; i++){
    cout << " " << arr[i];
}
    
    return 0;
}
